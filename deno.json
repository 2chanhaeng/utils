{
  "name": "@chomu/utils",
  "version": "0.1.12",
  "exports": "./mod.ts",
  "license": "MIT",
  "tasks": {
    "dev": "deno run --watch main.ts",
    "cov": "deno test --allow-all --coverage=cov/ && deno coverage cov/ && rm -rf cov",
    "lcov": "deno test --allow-all --coverage=cov/ && deno coverage --lcov cov/ > cov.lcov && rm -rf cov && genhtml -o html_cov cov.lcov && rm cov.lcov && open html_cov/index.html"
  },
  "imports": {
    "apply": "./apply/mod.ts",
    "array": "./array/mod.ts",
    "atom": "./atom/mod.ts",
    "bool": "./bool/mod.ts",
    "error": "./error/mod.ts",
    "date": "./date/mod.ts",
    "io": "./io/mod.ts",
    "iter": "./iter/mod.ts",
    "pipe": "./pipe/mod.ts",
    "pred": "./pred/mod.ts",
    "promise": "./promise/mod.ts",
    "record": "./record/mod.ts",
    "set": "./set/mod.ts",
    "str": "./str/mod.ts",
    "types": "./types/mod.ts",
    "@std/assert": "jsr:@std/assert@^1.0.13"
  },
  "publish": {
    "include": [
      "mod.ts",
      "apply",
      "array",
      "atom",
      "bool",
      "error",
      "date",
      "io",
      "iter",
      "pipe",
      "pred",
      "promise",
      "record",
      "set",
      "str",
      "types",
      "README.md"
    ],
    "exclude": ["**/*.test.ts"]
  }
}
